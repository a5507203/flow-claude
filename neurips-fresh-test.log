  "content": "\"\"\"\nTest suite for task-2-home-page-html\nValidates the index.html home page structure and content\n\"\"\"\n\nimport os\nimport re\nfrom pathlib import Path\nfrom html.parser import HTMLParser\n\n\nclass HTMLStructureParser(HTMLParser):\n    \"\"\"Custom HTML parser to extract structure information\"\"\"\n\n    def __init__(self):\n        super().__init__()\n        self.tags = []\n        self.sections = []\n        self.meta_tags = {}\n        self.title = \"\"\n        self.in_title = False\n\n    def handle_starttag(self, tag, attrs):\n        self.tags.append(tag)\n        attrs_dict = dict(attrs)\n\n        # Track sections with IDs or classes\n        if tag in ['header', 'nav', 'main', 'section', 'footer', 'div']:\n            self.sections.append({\n                'tag': tag,\n                'id': attrs_dict.get('id', ''),\n                'class': attrs_dict.get('class', '')\n            })\n\n        # Track meta tags\n        if tag == 'meta':\n            name = attrs_dict.get('name', attrs_dict.get('charset', attrs_dict.get('http-equiv', '')))\n            content = attrs_dict.get('content', attrs_dict.get('charset', ''))\n            self.meta_tags[name] = content\n\n        if tag == 'title':\n            self.in_title = True\n\n    def handle_endtag(self, tag):\n        if tag == 'title':\n            self.in_title = False\n\n    def handle_data(self, data):\n        if self.in_title:\n            self.title += data.strip()\n\n\ndef test_index_html_exists():\n    \"\"\"Test that index.html exists in the root directory\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    assert index_path.exists(), \"index.html must exist in the root directory\"\n    assert index_path.is_file(), \"index.html must be a file\"\n\n\ndef test_index_html_not_empty():\n    \"\"\"Test that index.html is not empty\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n    assert len(content.strip()) > 0, \"index.html must not be empty\"\n\n\ndef test_html5_doctype():\n    \"\"\"Test that index.html has proper HTML5 DOCTYPE\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    # Check for DOCTYPE (case insensitive)\n    assert re.search(r'<!DOCTYPE\\s+html>', content, re.IGNORECASE), \\\n        \"index.html must have HTML5 DOCTYPE declaration\"\n\n\ndef test_html_structure():\n    \"\"\"Test that index.html has basic HTML structure (html, head, body tags)\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    parser = HTMLStructureParser()\n    parser.feed(content)\n\n    assert 'html' in parser.tags, \"Document must have <html> tag\"\n    assert 'head' in parser.tags, \"Document must have <head> tag\"\n    assert 'body' in parser.tags, \"Document must have <body> tag\"\n\n\ndef test_meta_tags():\n    \"\"\"Test that required meta tags are present\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    parser = HTMLStructureParser()\n    parser.feed(content)\n\n    # Check for charset\n    has_charset = 'utf-8' in parser.meta_tags or 'UTF-8' in parser.meta_tags\n    assert has_charset, \"Document must specify UTF-8 charset\"\n\n    # Check for viewport meta tag\n    assert 'viewport' in parser.meta_tags, \\\n        \"Document must have viewport meta tag for responsive design\"\n\n\ndef test_title_tag():\n    \"\"\"Test that document has a title tag with NeurIPS content\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    parser = HTMLStructureParser()\n    parser.feed(content)\n\n    assert parser.title, \"Document must have a <title> tag with content\"\n    assert 'NeurIPS' in parser.title or 'neurips' in parser.title.lower(), \\\n        \"Title should mention NeurIPS conference\"\n\n\ndef test_semantic_html5_elements():\n    \"\"\"Test that document uses semantic HTML5 elements\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    parser = HTMLStructureParser()\n    parser.feed(content)\n\n    # Check for key semantic elements\n    assert 'header' in parser.tags, \"Document should use <header> element\"\n    assert 'main' in parser.tags, \"Document should use <main> element\"\n    assert 'footer' in parser.tags, \"Document should use <footer> element\"\n\n\ndef test_required_sections():\n    \"\"\"Test that all required content sections are present\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    # Case insensitive content checks\n    content_lower = content.lower()\n\n    # Check for NeurIPS 2028 mention\n    assert 'neurips' in content_lower, \"Page must mention NeurIPS\"\n    assert '2028' in content, \"Page must mention year 2028\"\n\n    # Check for navigation\n    assert '<nav' in content_lower, \"Page should have navigation element\"\n\n    # Check for hero/main content section\n    parser = HTMLStructureParser()\n    parser.feed(content)\n    has_hero = any('hero' in str(s.get('class', '')).lower() or\n                   'hero' in str(s.get('id', '')).lower()\n                   for s in parser.sections)\n    has_main = 'main' in parser.tags\n    assert has_hero or has_main, \\\n        \"Page should have a hero section or main content area\"\n\n\ndef test_conference_information_placeholders():\n    \"\"\"Test that placeholder content for conference info is present\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8').lower()\n\n    # Check for date/location placeholders or actual content\n    has_date_info = any(keyword in content for keyword in\n                        ['date', 'when', 'tbd', '2028'])\n    has_location_info = any(keyword in content for keyword in\n                           ['location', 'where', 'venue', 'tbd'])\n\n    assert has_date_info, \\\n        \"Page should mention dates or have date placeholder\"\n    assert has_location_info, \\\n        \"Page should mention location or have location placeholder\"\n\n\ndef test_css_link():\n    \"\"\"Test that document links to the CSS stylesheet\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    # Check for link to stylesheet\n    assert re.search(r'<link[^>]+href=[\"\\']css/styles\\.css[\"\\']', content, re.IGNORECASE), \\\n        \"Document should link to css/styles.css stylesheet\"\n\n\ndef test_no_inline_styles():\n    \"\"\"Test that document doesn't use excessive inline styles (should use external CSS)\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    # Count style attributes (some minimal inline styles are ok)\n    style_count = len(re.findall(r'style=', content, re.IGNORECASE))\n    assert style_count < 5, \\\n        \"Document should minimize inline styles (found {}, expected < 5)\".format(style_count)\n\n\ndef test_valid_html_basic():\n    \"\"\"Test basic HTML validity (matching tags, no obvious errors)\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    # Try to parse - will raise exception if malformed\n    parser = HTMLStructureParser()\n    try:\n        parser.feed(content)\n    except Exception as e:\n        assert False, f\"HTML parsing failed: {e}\"\n\n    # Check for common mistakes\n    assert not re.search(r'<<', content), \"Document contains double < characters\"\n    assert not re.search(r'>>', content), \"Document contains double > characters\"\n\n\ndef test_heading_hierarchy():\n    \"\"\"Test that document has proper heading hierarchy\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    # Should have at least an h1\n    assert re.search(r'<h1[^>]*>', content, re.IGNORECASE), \\\n        \"Document should have at least one <h1> heading\"\n\n\ndef test_footer_present():\n    \"\"\"Test that footer section exists with content\"\"\"\n    index_path = Path(__file__).parent / \"index.html\"\n    content = index_path.read_text(encoding='utf-8')\n\n    assert '<footer' in content.lower(), \"Document should have a <footer> element\"\n\n    # Footer should have some content\n    footer_match = re.search(r'<footer[^>]*>(.*?)</footer>', content,\n                            re.IGNORECASE | re.DOTALL)\n    if footer_match:\n        footer_content = footer_match.group(1).strip()\n        assert len(footer_content) > 10, \"Footer should have meaningful content\"\n\n\nif __name__ == \"__main__\":\n    # Run tests manually\n    import sys\n\n    tests = [\n        test_index_html_exists,\n        test_index_html_not_empty,\n        test_html5_doctype,\n        test_html_structure,\n        test_meta_tags,\n        test_title_tag,\n        test_semantic_html5_elements,\n        test_required_sections,\n        test_conference_information_placeholders,\n        test_css_link,\n        test_no_inline_styles,\n        test_valid_html_basic,\n        test_heading_hierarchy,\n        test_footer_present,\n    ]\n\n    failed = 0\n    passed = 0\n\n    for test_func in tests:\n        try:\n            test_func()\n            print(f\"PASS: {test_func.__name__}\")\n            passed += 1\n        except AssertionError as e:\n            print(f\"FAIL: {test_func.__name__}: {e}\")\n            failed += 1\n        except Exception as e:\n            print(f\"ERROR: {test_func.__name__}: {e}\")\n            failed += 1\n\n    print(f\"\\n{passed} passed, {failed} failed\")\n    sys.exit(0 if failed == 0 else 1)\n"
}
