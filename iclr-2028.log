  "content": "/**\n * ICLR 2028 - Schedule Page JavaScript\n * Handles interactive day tab switching for the schedule page\n */\n\n(function() {\n  'use strict';\n\n  /**\n   * Initialize the schedule page when DOM is fully loaded\n   */\n  function initSchedule() {\n    setupDayTabs();\n    setupKeyboardNavigation();\n    restoreActiveTab();\n  }\n\n  /**\n   * Setup day tab click handlers for switching between conference days\n   */\n  function setupDayTabs() {\n    const dayTabs = document.querySelectorAll('.day-tab');\n    const dayPanels = document.querySelectorAll('.day-panel');\n\n    if (!dayTabs.length || !dayPanels.length) {\n      return; // Exit if elements don't exist (not on schedule page)\n    }\n\n    dayTabs.forEach(tab => {\n      tab.addEventListener('click', function() {\n        const targetDay = this.getAttribute('data-day');\n        switchToDay(targetDay);\n        saveActiveTab(targetDay);\n      });\n    });\n  }\n\n  /**\n   * Switch to display a specific conference day\n   * @param {string} dayId - The day identifier (e.g., 'day1', 'day2')\n   */\n  function switchToDay(dayId) {\n    // Update tab states\n    const dayTabs = document.querySelectorAll('.day-tab');\n    dayTabs.forEach(tab => {\n      const isActive = tab.getAttribute('data-day') === dayId;\n      tab.classList.toggle('active', isActive);\n      tab.setAttribute('aria-selected', isActive.toString());\n    });\n\n    // Update panel visibility\n    const dayPanels = document.querySelectorAll('.day-panel');\n    dayPanels.forEach(panel => {\n      const isActive = panel.id === `${dayId}-panel`;\n      panel.classList.toggle('active', isActive);\n\n      if (isActive) {\n        panel.removeAttribute('hidden');\n      } else {\n        panel.setAttribute('hidden', '');\n      }\n    });\n\n    // Scroll to top of schedule content for better UX\n    const scheduleContent = document.querySelector('.schedule-content');\n    if (scheduleContent) {\n      const headerOffset = 80; // Account for sticky tabs\n      const elementPosition = scheduleContent.getBoundingClientRect().top;\n      const offsetPosition = elementPosition + window.pageYOffset - headerOffset;\n\n      window.scrollTo({\n        top: offsetPosition,\n        behavior: 'smooth'\n      });\n    }\n  }\n\n  /**\n   * Setup keyboard navigation for day tabs (arrow keys)\n   */\n  function setupKeyboardNavigation() {\n    const dayTabs = document.querySelectorAll('.day-tab');\n\n    dayTabs.forEach((tab, index) => {\n      tab.addEventListener('keydown', function(e) {\n        let targetIndex;\n\n        // Arrow key navigation\n        if (e.key === 'ArrowLeft' || e.key === 'ArrowUp') {\n          e.preventDefault();\n          targetIndex = index > 0 ? index - 1 : dayTabs.length - 1;\n        } else if (e.key === 'ArrowRight' || e.key === 'ArrowDown') {\n          e.preventDefault();\n          targetIndex = index < dayTabs.length - 1 ? index + 1 : 0;\n        } else if (e.key === 'Home') {\n          e.preventDefault();\n          targetIndex = 0;\n        } else if (e.key === 'End') {\n          e.preventDefault();\n          targetIndex = dayTabs.length - 1;\n        }\n\n        // Focus and activate target tab\n        if (targetIndex !== undefined) {\n          const targetTab = dayTabs[targetIndex];\n          targetTab.focus();\n          targetTab.click();\n        }\n      });\n    });\n  }\n\n  /**\n   * Save the active tab to localStorage for persistence across page reloads\n   * @param {string} dayId - The day identifier to save\n   */\n  function saveActiveTab(dayId) {\n    try {\n      localStorage.setItem('iclr2028-active-day', dayId);\n    } catch (e) {\n      // localStorage might not be available (private browsing, etc.)\n      console.warn('Unable to save active day to localStorage:', e);\n    }\n  }\n\n  /**\n   * Restore the previously active tab from localStorage\n   */\n  function restoreActiveTab() {\n    try {\n      const savedDay = localStorage.getItem('iclr2028-active-day');\n\n      if (savedDay) {\n        const savedTab = document.querySelector(`[data-day=\"${savedDay}\"]`);\n\n        // Only restore if the saved tab exists\n        if (savedTab) {\n          switchToDay(savedDay);\n        }\n      }\n    } catch (e) {\n      // localStorage might not be available\n      console.warn('Unable to restore active day from localStorage:', e);\n    }\n  }\n\n  /**\n   * Filter sessions by type (optional enhancement for future use)\n   * @param {string} sessionType - Type of session to filter (keynote, paper, workshop, poster)\n   */\n  function filterSessionsByType(sessionType) {\n    const sessionCards = document.querySelectorAll('.session-card');\n\n    sessionCards.forEach(card => {\n      if (sessionType === 'all') {\n        card.style.display = '';\n      } else {\n        const badge = card.querySelector('.session-type-badge');\n        if (badge && badge.classList.contains(`badge-${sessionType}`)) {\n          card.style.display = '';\n        } else if (sessionType === 'break' && card.classList.contains('session-break')) {\n          card.style.display = '';\n        } else {\n          card.style.display = 'none';\n        }\n      }\n    });\n  }\n\n  /**\n   * Add smooth scroll behavior to internal links\n   */\n  function setupSmoothScroll() {\n    document.querySelectorAll('a[href^=\"#\"]').forEach(anchor => {\n      anchor.addEventListener('click', function(e) {\n        const href = this.getAttribute('href');\n\n        // Skip if it's just \"#\" or empty\n        if (!href || href === '#') return;\n\n        e.preventDefault();\n        const target = document.querySelector(href);\n\n        if (target) {\n          target.scrollIntoView({\n            behavior: 'smooth',\n            block: 'start'\n          });\n        }\n      });\n    });\n  }\n\n  /**\n   * Export functions for potential external use\n   */\n  window.ICLRSchedule = {\n    switchToDay: switchToDay,\n    filterSessionsByType: filterSessionsByType\n  };\n\n  // Initialize when DOM is ready\n  if (document.readyState === 'loading') {\n    document.addEventListener('DOMContentLoaded', initSchedule);\n  } else {\n    initSchedule();\n  }\n\n})();\n"
